<h2>Lista de Projetos Cadastrados</h2>

<table class="table table-hover">
  <thead>
    <tr>
      <th>Nome</th>
      <th></th>
    </tr>
  </thead>
    <tbody>
      <% projects.forEach(function(project) { %>
		  <tr>
		    <td><%= project.name %></td>
		    <td><a href="/projects/<%= project.id %>" class="btn btn-info"><i class="icon-plus-sign"></i></a></td>
		  </tr>
	  <% }) %>
    </tbody>
</table>